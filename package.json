{
  "name": "@unsoon/qdrant-client",
  "version": "1.0.0",
  "description": "Qdrant client for NestJS with gRPC and REST support",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unsoon/qdrant-client.git"
  },
  "homepage": "https://github.com/unsoon/qdrant-client#readme",
  "bugs": {
    "url": "https://github.com/unsoon/qdrant-client/issues"
  },
  "keywords": [
    "qdrant",
    "nestjs",
    "vector",
    "database",
    "client",
    "grpc",
    "rest",
    "unsoon"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "packageManager": "pnpm@9.1.2",
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@qdrant/js-client-grpc": "^1.14.0",
    "@qdrant/js-client-rest": "^1.14.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.0",
    "tsup": "^6.1.3",
    "typescript": "^5.1.6"
  }
}
